{
	"name": "@mvsde/eleventy-plugin-vue",
	"version": "0.1.0",
	"description": "Vue 3 templates for Eleventy.",
	"license": "MIT",
	"type": "module",
	"main": "dist/main.cjs",
	"scripts": {
		"dev": "rollup --config rollup.config.js --watch",
		"build": "rollup --config rollup.config.js",
		"lint": "eslint \"**/*.js\"",
		"test": "node --test test/unit",
		"prepack": "npm run build",
		"version": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file && git add CHANGELOG.md"
	},
	"files": [
		"main.js",
		"lib",
		"dist"
	],
	"peerDependencies": {
		"vue": "^3"
	},
	"dependencies": {
		"@vitejs/plugin-vue": "^3.2.0-beta.0",
		"module-from-string": "^3.3.0",
		"rollup": "^3.2.1",
		"rollup-plugin-css-only": "^3.1.0"
	},
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0-canary.16",
		"conventional-changelog-cli": "^2.2.2",
		"eslint": "^8.25.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-vue": "^9.6.0"
	}
}
